{
    "version": 2,
    "env": {
        "DATABASE_URL": "@db_uri",
        "JWT_SECRET": "@secret",
        "JWT_EXP": "@exp"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}